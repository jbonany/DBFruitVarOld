<h1>Listing varieties</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Cultivar</th>
    <th>Clon</th>
    <th>Clone</th>
    <th>Synonim</th>
    <th>Country</th>
    <th>Breeder idmale parent</th>
    <th>Female parent</th>
    <th>Year</th>
    <th>Group</th>
    <th>Observations cat</th>
    <th>Observations esp</th>
    <th>Observations eng</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @varieties.each do |variety| %>
  <tr>
    <td><%= variety.name %></td>
    <td><%= variety.cultivar %></td>
    <td><%= variety.clon %></td>
    <td><%= variety.clone %></td>
    <td><%= variety.synonim %></td>
    <td><%= variety.country_id %></td>
    <td><%= variety.breeder_idmale_parent %></td>
    <td><%= variety.female_parent %></td>
    <td><%= variety.year %></td>
    <td><%= variety.group_id %></td>
    <td><%= variety.observations_cat %></td>
    <td><%= variety.observations_esp %></td>
    <td><%= variety.observations_eng %></td>
    <td><%= link_to 'Show', variety %></td>
    <td><%= link_to 'Edit', edit_variety_path(variety) %></td>
    <td><%= link_to 'Destroy', variety, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Variety', new_variety_path %>
